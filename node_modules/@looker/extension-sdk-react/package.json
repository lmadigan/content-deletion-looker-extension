{
  "name": "@looker/extension-sdk-react",
  "version": "0.2.0",
  "description": "Looker Extension SDK for React",
  "main": "lib/index",
  "scripts": {
    "test": "jest",
    "prepare": "npm run build --scripts-prepend-node-path",
    "build": "shx rm -rf lib && tsc -p tsconfig.build.json && babel src --out-dir lib --extensions \".ts,.tsx,.js,.jsx\" -s",
    "deploy:1": "shx rm -rf ~/looker/routing-sample/node_modules/@looker/extension-sdk-react/lib && cp -rf lib ~/looker/routing-sample/node_modules/@looker/extension-sdk-react",
    "deploy:2": "shx rm -rf ~/looker/extension-template/node_modules/@looker/extension-sdk-react/lib && cp -rf lib ~/looker/extension-template/node_modules/@looker/extension-sdk-react",
    "deploy": "npm run \"deploy:1\" --scripts-prepend-node-path && npm run \"deploy:2\" --scripts-prepend-node-path "
  },
  "author": "Looker",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "ts-loader": "^6.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.3",
    "@looker/extension-sdk": "0.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.5",
    "@types/react": "16.9.1",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "^5.1.0",
    "@types/readable-stream": "^2.3.5",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "jest": "^24.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.1.2",
    "shx": "^0.3.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "@looker/extension-sdk": "0.2.0",
    "@looker/sdk": "^0.2.0-beta.23",
    "@types/react": "^16.8",
    "@types/react-dom": "^16.8",
    "react": "^16.8",
    "react-dom": "^16.8",
    "react-router-dom": "^5.1.2"
  }
}
